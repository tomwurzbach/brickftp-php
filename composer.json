{
  "name": "tomwurzbach/files-php",
  "type": "project",
  "description": "Files.com PHP library",
  "keywords": [
    "brickftp", "files", "files.com", "brickftp.com"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.3",
    "psy/psysh": "^0.9.9"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrickFTP\\": "lib/"
    },
    "classmap": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
